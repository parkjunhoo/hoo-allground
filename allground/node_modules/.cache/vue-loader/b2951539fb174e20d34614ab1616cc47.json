ol class=\"d-flex justify-center mt-5\" cols=\"12\" v-if=\"this.$vuetify.breakpoint.smAndDown\">\r\n              <v-card @click=\"dialog=true\" class=\"mx-auto\"><v-img :width=\"storeWidth\" src=\"@/assets/service/services/windowstore.png\"></v-img></v-card>\r\n              <v-card @click=\"dialog=true\" class=\"mx-auto\"><v-img :width=\"storeWidth\" src=\"@/assets/service/services/googlestore.png\"></v-img></v-card>\r\n              <v-card @click=\"dialog=true\" class=\"mx-auto\"><v-img :width=\"storeWidth\" src=\"@/assets/service/services/applestore.png\"></v-img></v-card>\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n      </v-img>\r\n    </v-carousel-item>\r\n\r\n\r\n  </v-carousel>\r\n  <!-- <v-container style=\"padding-top:70px;\">\r\n    <p style=\"color:white;\">메인</p>\r\n  </v-container> -->\r\n\r\n   <v-dialog\r\n      v-model=\"dialog\"\r\n      width=\"50vw\"\r\n      height=\"30vh\"\r\n    >\r\n      <v-card class=\"pa-3\" color=\"transparent\" elevation=\"10\" width=\"100%\" height=\"100%\">\r\n        <v-row>\r\n          <v-col style=\"background-color:rgba(55,100,55,.9)\" cols=\"12\">\r\n            <p class=\"titleSubText\" style=\"text-align:center; font-family: 'Noto Sans KR', cursive;\">Coming soon</p>\r\n          </v-col>\r\n        </v-row>\r\n        <v-row style=\"background-color:rgba(0,0,0,.8)\" class=\"d-flex justify-center align-center fill-height\">\r\n          <v-col class=\"d-flex justify-center\" cols=\"12\">\r\n            <p class=\"subText\" style=\"text-align:center\">현재 서비스 준비중 입니다.</p>\r\n          </v-col>\r\n          <v-col class=\"d-flex justify-center\" cols=\"12\">\r\n            <v-btn @click=\"dialog=false;\" color=\"white\" outlined>close</v-btn>\r\n          </v-col>\r\n        </v-row>\r\n      </v-card>\r\n    </v-dialog>\r\n    \r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport bus from '@/utils/bus.js'\r\nexport default {\r\n  data(){\r\n    return{\r\n      dialog:false,\r\n      carouselIndex:0,\r\n      scrollReady:2,\r\n      servicesImg:[\r\n        require('@/assets/service/services/1.png'),\r\n        require('@/assets/service/services/2.png'),\r\n        require('@/assets/service/services/3.png'),\r\n        require('@/assets/service/services/4.png'),\r\n        require('@/assets/service/services/5.png'),\r\n        require('@/assets/service/services/6.png'),\r\n      ],\r\n      preloadImg:[\r\n        require('@/assets/service/1.jpg'),\r\n        require('@/assets/service/2.jpg'),\r\n        require('@/assets/service/services/1.png'),\r\n        require('@/assets/service/services/2.png'),\r\n        require('@/assets/service/services/3.png'),\r\n        require('@/assets/service/services/4.png'),\r\n        require('@/assets/service/services/5.png'),\r\n        require('@/assets/service/services/6.png'),\r\n      ],\r\n    }\r\n  },\r\n  created(){\r\n    this.preload();\r\n  },\r\n  mounted(){\r\n    this.preload();\r\n    this.$store.commit('set_showTipMessage',true);\r\n    this.$nextTick(()=>{\r\n      setTimeout( ()=>{bus.$emit('end:loading')},1000 ); // 로딩이 끝났다는걸 알려주기위한 마운트 된 후 비동기처리 셋타이머 ms인자 500때문에 기본딜레이 500잇음 0하면 페이지 마운트후 로딩) //\r\n    });\r\n  },\r\n  computed:{\r\n    Transition(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return \"slide-y-transition\";\r\n        case 'sm' : return \"slide-x-transition\";\r\n        case 'md' : return \"slide-x-transition\";\r\n        case 'lg' : return \"slide-x-transition\";\r\n        case 'xl' : return \"slide-x-transition\";\r\n        default : return \"slide-x-transition\";\r\n      }\r\n    },\r\n    reverseTransition(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return \"slide-y-reverse-transition\";\r\n        case 'sm' : return \"slide-x-reverse-transition\";\r\n        case 'md' : return \"slide-x-reverse-transition\";\r\n        case 'lg' : return \"slide-x-reverse-transition\";\r\n        case 'xl' : return \"slide-x-reverse-transition\";\r\n        default : return \"slide-x-reverse-transition\";\r\n      }\r\n    },\r\n    imgWidth(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '30vw';\r\n        case 'sm' : return '30vw';\r\n        case 'md' : return '10vw';\r\n        case 'lg' : return '10vw';\r\n        case 'xl' : return '10vw';\r\n        default : return '10vw';\r\n      }\r\n    },\r\n    storeWidth(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '28vw';\r\n        case 'sm' : return '25vw';\r\n        case 'md' : return '8vw';\r\n        case 'lg' : return '8vw';\r\n        case 'xl' : return '8vw';\r\n        default : return '8vw';\r\n      }\r\n    },\r\n    imgHeight(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '30vw';\r\n        case 'sm' : return '30vw';\r\n        case 'md' : return '10vw';\r\n        case 'lg' : return '10vw';\r\n        case 'xl' : return '10vw';\r\n        default : return '10vw';\r\n      }\r\n    },\r\n    iconWitdh(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '110';\r\n        case 'sm' : return '110';\r\n        case 'md' : return '140';\r\n        case 'lg' : return '170';\r\n        case 'xl' : return '170';\r\n        default : return '170';\r\n      }\r\n    },\r\n    tableHeight(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '11vh';\r\n        case 'sm' : return '11vh';\r\n        case 'md' : return '15vh';\r\n        case 'lg' : return '17vh';\r\n        case 'xl' : return '17vh';\r\n        default : return '17vh';\r\n      }\r\n    },\r\n    tableWidth(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '75vw';\r\n        case 'sm' : return '75vw';\r\n        case 'md' : return '75vw';\r\n        case 'lg' : return '75vw';\r\n        case 'xl' : return '75vw';\r\n        default : return '75vw';\r\n      }\r\n    },\r\n    delimitersLocation(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return \"right\";\r\n        case 'sm' : return \"right\";\r\n        case 'md' : return null;\r\n        case 'lg' : return null;\r\n        case 'xl' : return null;\r\n        default : return null;\r\n      }\r\n    },\r\n    mainServiceImgWidth1(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '230';\r\n        case 'sm' : return '300';\r\n        case 'md' : return '480';\r\n        case 'lg' : return '530';\r\n        case 'xl' : return '630';\r\n        default : return '630';\r\n      }\r\n    },\r\n    mainServiceImgWidth2(){\r\n      switch(this.$vuetify.breakpoint.name){\r\n        case 'xs' : return '280';\r\n        case 'sm' : return '300';\r\n        case 'md' : return '480';\r\n        case 'lg' : return '530';\r\n        case 'xl' : return '630';\r\n        default : return '630';\r\n      }\r\n    },\r\n  },\r\n  methods:{\r\n\r\n    preload(){\r\n      for(var i = 0; i<this.preloadImg.length; i++)\r\n      {\r\n        var preloadImage = new Image();\r\n        preloadImage.src = this.preloadImg[i]\r\n      }\r\n    },\r\n    carouselTouchDown(){\r\n      if(this.areaFocus) return;\r\n      if(this.carouselIndex===0) return;\r\n      if(this.carouselIndex<=2){\r\n        this.$store.commit('set_showTipMessage',false);\r\n        this.carouselIndex--;\r\n      } \r\n    },\r\n    carouselTouchUp(){\r\n      if(this.areaFocus) return;\r\n      if(this.carouselIndex===2) return; \r\n      {\r\n        this.$store.commit('set_showTipMessage',false);\r\n        if(this.carouselIndex>=0) this.carouselIndex++;\r\n      }\r\n    },\r\n    carouselScroll(e){\r\n      if(this.scrollReady>0)\r\n      {\r\n        this.$store.commit('set_showTipMessage',false);\r\n        if(e.wheelDelta>0){\r\n          this.carouselIndex--;\r\n          this.scrollReady=0;\r\n        }\r\n        else{\r\n          if(this.carouselIndex!=2)\r\n          {\r\n          this.carouselIndex++;\r\n          this.scrollReady=0;\r\n          }\r\n        }\r\n      }\r\n      else this.scrollReady++;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"../assets/css/unify.css\");\r\n\r\n.serviceTitle {\r\n  font-size: 2.3rem;\r\n  font-family: 'Russo One', sans-serif;\r\n}\r\n.serviceSubTitle {\r\n  font-size: 1.3rem;\r\n  font-family: 'Noto Sans KR';\r\n}\r\n.titleSubText {\r\n  font-size: 1.8rem;\r\n  font-family: 'Noto Sans KR';\r\n  padding: 7px 0;\r\n}\r\n.subText {\r\n  font-size: 1.3rem;\r\n  color: white;\r\n}\r\n@media (max-width:1000px){\r\n  .serviceTitle {\r\n    font-size: 2.3rem;\r\n    font-family: 'Russo One', sans-serif;\r\n  }\r\n  .serviceSubTitle {\r\n    font-size: 1.3rem;\r\n    font-family: 'Noto Sans KR';\r\n  }\r\n  .titleSubText {\r\n    font-size: 1.8rem;\r\n    font-family: 'Noto Sans KR';\r\n    padding: 7px 0;\r\n  }\r\n}\r\n@media (max-width:680px){\r\n  .serviceTitle {\r\n    font-size: 2.3rem;\r\n    font-family: 'Russo One', sans-serif;\r\n  }\r\n  .serviceSubTitle {\r\n    font-size: 1.3rem;\r\n    font-family: 'Noto Sans KR';\r\n  }\r\n  .titleSubText {\r\n    font-size: 1.8rem;\r\n    font-family: 'Noto Sans KR';\r\n    padding: 7px 0;\r\n  }\r\n}\r\n@media (max-width:520px){\r\n  .serviceTitle {\r\n    font-size: 1.6rem;\r\n    font-family: 'Russo One', sans-serif;\r\n  }\r\n  .serviceSubTitle {\r\n    font-size: 1.1rem;\r\n    font-family: 'Noto Sans KR';\r\n  }\r\n  .titleSubText {\r\n    font-size: 1rem;\r\n    font-family: 'Noto Sans KR';\r\n    padding: 7px 0;\r\n  }\r\n  .subText {\r\n    font-size: .9rem;\r\n    color: white;\r\n  }\r\n  .tableSubText {\r\n    font-size: .8rem;\r\n    font-family: 'Noto Sans KR';\r\n  }\r\n}\r\n</style>"]}]}