ents: {\r\n    EditorContent,\r\n    MenuBar,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      editor: null,\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.editor = new Editor({\r\n      extensions: [\r\n        [new CustomStyle()],\r\n        StarterKit,\r\n        Highlight,\r\n        TaskList,\r\n        TaskItem,\r\n        CharacterCount.configure({\r\n          limit: 90000,\r\n        }),\r\n      ],\r\n    })\r\n  },\r\n\r\n\r\n  methods: {\r\n  },\r\n\r\n  beforeDestroy() {\r\n    this.editor.destroy()\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.editor {\r\n  display: flex;\r\n  flex-direction: column;\r\n  max-height: 400px;\r\n  color: #0D0D0D;\r\n  background-color: white;\r\n  border: 3px solid #0D0D0D;\r\n  border-radius: 0.75rem;\r\n\r\n  &__header {\r\n    display: flex;\r\n    align-items: center;\r\n    flex: 0 0 auto;\r\n    flex-wrap: wrap;\r\n    padding: 0.25rem;\r\n    border-bottom: 3px solid #0D0D0D;\r\n  }\r\n\r\n  &__content {\r\n    padding: 1.25rem 1rem;\r\n    flex: 1 1 auto;\r\n    overflow-x: hidden;\r\n    overflow-y: auto;\r\n    -webkit-overflow-scrolling: touch;\r\n  }\r\n\r\n  &__footer {\r\n    display: flex;\r\n    flex: 0 0 auto;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n    white-space: nowrap;\r\n    border-top: 3px solid #0D0D0D;\r\n    font-size: 12px;\r\n    font-weight: 600;\r\n    color: #0D0D0D;\r\n    white-space: nowrap;\r\n    padding: 0.25rem 0.75rem;\r\n  }\r\n\r\n  /* Some information about the status */\r\n  &__status {\r\n    display: flex;\r\n    align-items: center;\r\n    border-radius: 5px;\r\n\r\n    &::before {\r\n      content: ' ';\r\n      flex: 0 0 auto;\r\n      display: inline-block;\r\n      width: 0.5rem;\r\n      height: 0.5rem;\r\n      background: rgba(#0D0D0D, 0.5);\r\n      border-radius: 50%;\r\n      margin-right: 0.5rem;\r\n    }\r\n\r\n    &--connecting::before {\r\n      background: #616161;\r\n    }\r\n\r\n 