CAAA,UAAA,CAAA,IAAA;AACA,aAAA,IAAA;AAAA,iBAAA,KAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,KAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,KAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,KAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,KAAA;;AACA;AAAA,iBAAA,KAAA;AANA;AAQA,KApBA;AAqBA,IAAA,UArBA,wBAqBA;AACA,cAAA,KAAA,QAAA,CAAA,UAAA,CAAA,IAAA;AACA,aAAA,IAAA;AAAA,iBAAA,IAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,IAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,IAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,IAAA;;AACA,aAAA,IAAA;AAAA,iBAAA,IAAA;;AACA;AAAA,iBAAA,IAAA;AANA;AAQA;AA9BA;AAfA,CAAA","sourcesContent":["<template>\r\n<div id=\"header\">\r\n    <v-app-bar\r\n    :height=\"headerHeight\"\r\n    color=\"transparent\"\r\n    flat\r\n    style=\"z-index:5;\"\r\n    >\r\n        <v-row class=\"d-flex align-center\">\r\n            <v-col class=\"d-flex align-center\" cols=\"auto\">\r\n                <router-link to=\"/\"><v-card :ripple=\"false\" class=\"transparent\" elevation=\"0\" :width=\"logoWidth\" :height=\"logoHegiht\"><v-img contain width=\"100%\" height=\"100%\" src=\"@/assets/logo/white_wide_logo_resize.png\" ></v-img></v-card></router-link>\r\n            </v-col>\r\n            <v-spacer></v-spacer>\r\n            <v-col v-if=\"!this.$vuetify.breakpoint.smAndDown\" cols=\"auto\">\r\n                <v-tabs hide-slider fixed-tabs color=\"white\" background-color=\"transparent\">\r\n                    <v-tab :to=\"i.to\" class=\"headerTab mx-5\" :ripple=\"false\" v-for=\"i in adminMenuList\" :key=\"i.title\">\r\n                        <p style=\"white-space:nowrap;\" class=\"headerTitle\">{{i.title}}</p>\r\n                    </v-tab>\r\n                </v-tabs>\r\n            </v-col>\r\n            <v-spacer></v-spacer>\r\n            <v-col v-if=\"!this.$vuetify.breakpoint.smAndDown\" cols=\"auto\">\r\n                <p class=\"headerText\">LOGOUT</p>\r\n            </v-col>\r\n            <v-col v-if=\"this.$vuetify.breakpoint.smAndDown\" cols=\"auto\">\r\n                <v-app-bar-nav-icon color=\"white\" @click=\"OpenDrawer\"></v-app-bar-nav-icon>\r\n            </v-col>\r\n            <hr style=\"background-color:white; opacity:.3; position:absolute; width:100%; left:0px; top:100%; height:1px;\">\r\n        </v-row>\r\n    </v-app-bar>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            adminMenuList:[\r\n                {title:'컨텐츠관리', to:'edit_contents'},\r\n                {title:'메뉴 관리', to:'edit_menu'},\r\n                {title:'게시판 관리', to:'edit_board'},\r\n            ],\r\n        }\r\n    },\r\n    methods:{\r\n        OpenDrawer(){\r\n            this.$store.commit('set_drawerBool',!this.$store.state.drawerBool);\r\n        },\r\n    },\r\n    computed:{\r\n        headerHeight(){\r\n            switch(this.$vuetify.breakpoint.name){\r\n            case 'xs' : return \"35\";\r\n            case 'sm' : return \"35\";\r\n            case 'md' : return \"60\";\r\n            case 'lg' : return \"60\";\r\n            case 'xl' : return \"60\";\r\n            default : return \"60\";\r\n            }\r\n        },\r\n        logoWidth(){\r\n            switch(this.$vuetify.breakpoint.name){\r\n            case 'xs' : return \"120\";\r\n            case 'sm' : return \"150\";\r\n            case 'md' : return \"150\";\r\n            case 'lg' : return \"150\";\r\n            case 'xl' : return \"150\";\r\n            default : return \"150\";\r\n            }\r\n        },\r\n        logoHegiht(){\r\n            switch(this.$vuetify.breakpoint.name){\r\n            case 'xs' : return \"30\";\r\n            case 'sm' : return \"30\";\r\n            case 'md' : return \"60\";\r\n            case 'lg' : return \"60\";\r\n            case 'xl' : return \"60\";\r\n            default : return \"60\";\r\n            }\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n#header .v-card--link:before , .v-tab:before {\r\n    background-color: transparent !important;\r\n}\r\n</style>\r\n\r\n<style scoped>\r\n.headerHolder{\r\n    max-width:1400px;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.headerTitle{\r\n    transition:800ms ease all;\r\n    font-size:16px;\r\n    font-family: 'Noto Sans KR', sans-serif;\r\n    color:rgba(255,255,255,.7);\r\n}\r\n\r\n.v-tab:hover .hoverspan {\r\n    width:80%;\r\n    opacity:1;\r\n}\r\n.hoverspan{\r\n    background-color:white;\r\n    bottom:3px; left:10%; width:0; height:2px;\r\n    position:absolute;\r\n    transition: all .8s;\r\n    z-index:2;\r\n    opacity:0;\r\n}\r\n.activespan{\r\n    background-color:white;\r\n    bottom:3px; left:10%; width:0; height:2px;\r\n    position:absolute;\r\n    transition: all .8s;\r\n    z-index:2;\r\n    opacity:0;\r\n}\r\n\r\n.headerTab:hover .headerTitle{\r\n    color:white;\r\n}\r\n</style>"],"sourceRoot":"src/components/Header"}]}