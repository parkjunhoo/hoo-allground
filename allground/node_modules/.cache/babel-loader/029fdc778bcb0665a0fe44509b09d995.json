AAA,CAAA,gBAAA,EAAA;AACA,QAAA,EAAA,EAAA,KAAA,MAAA,CAAA,EADA;AAEA,QAAA,KAAA,EAAA,KAAA,KAFA;AAGA,QAAA,QAAA,EAAA,KAAA;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,gBAAA;;AACA,qBAAA,KAAA,CAAA,aAAA;;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,cAAA,EAAA,CAAA;AACA,OAVA;AAYA,KAfA;AAgBA,IAAA,WAhBA,yBAgBA;AAAA;;AACA,qBAAA,IAAA,CAAA,kBAAA,EAAA;AACA,QAAA,EAAA,EAAA,KAAA,MAAA,CAAA;AADA,OAAA,EAGA,IAHA,CAGA,YAAA;AACA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,gBAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,cAAA,EAAA,CAAA;AACA,OAPA;AAQA,KAzBA;AA0BA,IAAA,WA1BA,yBA0BA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,WAAA,KAAA,CAAA,cAAA,EAAA,CAAA;AACA;AA7BA;AApCA,C","sourcesContent":["<template>\r\n    <div id=\"app\">\r\n      <v-text-field v-model=\"title\" background-color=\"green darken-1\" label=\"제목\" solo hide-details></v-text-field>\r\n      <jodit-editor v-model=\"content\" />\r\n      <div class=\"grey d-flex justify-center\">\r\n        <v-btn @click=\"clickSubmit\" width=\"100px\" color=\"green\">등록하기</v-btn>\r\n        <v-btn @click=\"clickDelete\" width=\"100px\" color=\"pink\">삭제</v-btn>\r\n        <v-btn @click=\"clickCancel\" width=\"100px\" color=\"red\">취소</v-btn>\r\n      </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport 'jodit/build/jodit.min.css'\r\nimport { JoditEditor } from 'jodit-vue'\r\nimport axios from 'axios'\r\nimport bus from '@/utils/bus.js'\r\n\r\nexport default {\r\n    name: 'JoditEdit',\r\n    components: { JoditEditor },\r\n    props: {\r\n      output: {\r\n        type: Object,\r\n      }\r\n    },\r\n    data () {\r\n        return {\r\n            title:'',\r\n            content: '',\r\n        }\r\n    },\r\n    mounted(){\r\n      this.title = this.output.title;\r\n      this.content = this.output.contents;\r\n    },\r\n    watch:{\r\n      outputId(){\r\n        // console.log('work');\r\n        this.title = this.output.title;\r\n        this.content = this.output.contents;\r\n      },\r\n    },\r\n    computed:{\r\n      outputId(){\r\n        return this.output.id;\r\n      },\r\n      outputTitle(){\r\n        return this.output.title;\r\n      },\r\n      outputContents(){\r\n        return this.output.contents;\r\n      },\r\n    },\r\n    methods: {\r\n      clickSubmit(){\r\n        bus.$emit('start:loading');\r\n        axios.put('api/board/edit',{\r\n          id:this.output.id,\r\n          title: this.title,\r\n          contents: this.content\r\n        })\r\n        .then(()=>{\r\n          // console.log(res.data);\r\n          this.$store.dispatch('get_board_find');\r\n          bus.$emit('end:loading');\r\n          this.$emit('carouselMove',0);\r\n        })\r\n        \r\n      },\r\n      clickDelete(){\r\n        axios.post('api/board/delete',{\r\n          id:this.output.id,\r\n        })\r\n        .then(()=>{\r\n          // console.log(res.data);\r\n          this.$store.dispatch('get_board_find');\r\n          this.$emit('carouselMove',0);\r\n        })\r\n      },\r\n      clickCancel(){\r\n        window.location.reload();\r\n        this.$emit('carouselMove',0);\r\n      },\r\n    },\r\n}\r\n</script>"],"sourceRoot":"src/components/Jodit"}]}