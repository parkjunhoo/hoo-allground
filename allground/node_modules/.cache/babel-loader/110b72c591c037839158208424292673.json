cols=\"2\">\r\n                                        <p class=\"subText\">날짜</p>\r\n                                    </v-col>\r\n                                </v-row>\r\n                                <v-row v-for=\"i in boards\" :key=\"i\">\r\n                                    <v-col class=\"blue\" cols=\"10\">\r\n                                        <p style=\"overflow:hidden;\" class=\"subText\">{{i.title}}</p>\r\n                                    </v-col>\r\n                                    <v-col class=\"blue lighten-2\" cols=\"2\">\r\n                                        <p class=\"subText\">{{i.regTime.slice(0,10)}}</p>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-container>\r\n                        </div>\r\n                    </v-carousel-item>\r\n\r\n                    <v-carousel-item>\r\n                        <div class=\"scrollDiv\" style=\"height:100%;\">\r\n                            <v-container class=\"viewContainer d-flex\">\r\n                                <v-row>\r\n                                    <v-col cols=\"12\">\r\n                                        <TipTab/>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-container>\r\n                        </div>\r\n                    </v-carousel-item>\r\n\r\n\r\n                </v-carousel>\r\n            </v-col>\r\n        </v-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { generateHTML } from '@tiptap/html'\r\nimport bus from '@/utils/bus.js'\r\nimport AdminDrawer from '@/components/Admin/AdminDrawer.vue'\r\nimport TipTab from '@/components/TipTap/TipTap.vue'\r\nimport axios from 'axios'\r\nimport Document from '@tiptap/extension-document'\r\nimport Paragraph from '@tiptap/extension-paragraph'\r\nimport Text from '@tiptap/extension-text'\r\nimport Bold from '@tiptap/extension-bold'\r\n\r\nexport default {\r\n    components:{\r\n        AdminDrawer,\r\n        TipTab,\r\n    },\r\n    data(){\r\n        return{\r\n            today:'',\r\n            userData:'',\r\n            carouselIndex:0,\r\n            output:null,\r\n        }\r\n    },\r\n    mounted() {\r\n        axios.get('api/board/find')\r\n        .then((res)=>{\r\n            this.board = res.data[0].contents;\r\n            console.log(this.board);\r\n            this.output = generateHTML(this.board,[\r\n                Document,\r\n                Paragraph,\r\n                Text,\r\n                Bold,\r\n            ])\r\n        })\r\n        this.$store.dispatch('get_board_find');\r\n        setInterval(this.t,1000);\r\n        this.$store.commit('set_adminPage',true);\r\n        this.$store.commit('set_showTipMessage',false);\r\n        this.$nextTick(()=>{\r\n        setTimeout( ()=>{bus.$emit('end:loading')},1500 );\r\n        });\r\n    },\r\n    computed:{\r\n        aDrawerBool:{\r\n            get(){\r\n                return this.$store.state.Admin.aDrawerBool;\r\n            },\r\n            set(bool){\r\n                this.$store.commit('set_aDrawerBool',bool);\r\n            },\r\n        },\r\n        boards(){\r\n            return this.$store.state.Board.boards;\r\n        },\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"../assets/css/unify.css\");\r\n</style>\r\n"],"sourceRoot":"src/views"}]}